{
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxSingleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "parser": "babel"
            }
        },
        {
            "files": "*.jsx",
            "options": {
                "parser": "babel"
            }
        }
    ]
}
